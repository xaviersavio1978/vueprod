{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "networking": {
  "allowedIpRanges": ["AzureFrontDoor.Backend"]
},
    "forwardingGateway": {
  "requiredHeaders": {
    "X-Azure-FDID" : "<YOUR-FRONT-DOOR-ID>"
  },
  "allowedForwardedHosts": [
    "my-sitename.azurefd.net"
  ]
},
    {
    "route": "/members",
    "allowedRoles": ["authenticated, members"],
    "headers": {
        "Cache-Control": "no-store"
    }
}
}
